{
	"name": "@haneullabs/walrus",
	"version": "0.9.0",
	"private": false,
	"description": "Walrus SDK",
	"license": "Apache-2.0",
	"author": "Haneul Labs <build@haneullabs.com>",
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"sideEffects": false,
	"files": [
		"CHANGELOG.md",
		"dist"
	],
	"scripts": {
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"build": "build-package",
		"test": "NODE_OPTIONS='--expose-gc' vitest run",
		"codegen": "haneul-ts-codegen generate && pnpm lint:fix",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"dev:write-from-wallet": "vite examples/write-from-wallet",
		"dev:benchmark": "vite examples/benchmark"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeunhwaJeong/haneul-ts-sdks.git"
	},
	"bugs": {
		"url": "https://github.com/haneullabs/ts-sdks/issues"
	},
	"homepage": "https://sdk.haneullabs.com/walrus",
	"devDependencies": {
		"@hono/node-server": "^1.19.6",
		"@haneullabs/build-scripts": "workspace:*",
		"@haneullabs/codegen": "workspace:*",
		"@haneullabs/dapp-kit-core": "workspace:*",
		"@haneullabs/dapp-kit-react": "workspace:*",
		"@tanstack/react-query": "^5.90.11",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.1",
		"hono": "^4.10.7",
		"react": "^19.2.1",
		"react-dom": "^19.2.1",
		"typescript": "^5.9.3",
		"undici": "^7.16.0",
		"vite": "^7.2.6",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@haneullabs/bcs": "workspace:*",
		"@haneullabs/haneul": "workspace:*",
		"@haneullabs/utils": "workspace:*",
		"@haneullabs/walrus-wasm": "workspace:*",
		"dataloader": "^2.2.3"
	}
}
