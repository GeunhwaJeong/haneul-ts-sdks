{
	"name": "@haneullabs/kiosk",
	"author": "Haneul Labs <build@haneullabs.com>",
	"description": "Haneul Kiosk library",
	"version": "0.14.6",
	"license": "Apache-2.0",
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"sideEffects": false,
	"files": [
		"CHANGELOG.md",
		"dist",
		"src"
	],
	"engines": {
		"node": ">=18"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HaneulLabs/ts-sdks.git"
	},
	"bugs": {
		"url": "https://github.com/HaneulLabs/ts-sdks/issues/new"
	},
	"scripts": {
		"build": "build-package",
		"test": "echo 'No unit tests for kiosk SDK'",
		"pre-commit": "pnpm prettier:fix && pnpm lint && pnpm build",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"test:e2e": "vitest run e2e"
	},
	"dependencies": {
		"@haneullabs/haneul": "workspace:*",
		"@haneullabs/utils": "workspace:*"
	},
	"devDependencies": {
		"@haneullabs/build-scripts": "workspace:*",
		"cross-env": "^10.1.0",
		"tmp": "^0.2.5",
		"ts-retry-promise": "^0.8.1",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^4.0.15",
		"wait-on": "^9.0.3"
	}
}
