{
	"name": "@haneullabs/mvr-static",
	"author": "Haneul Labs <build@haneullabs.com>",
	"description": "The static generation tool for Move Registry (mvr)",
	"homepage": "https://sdk.haneullabs.com",
	"version": "0.2.12",
	"license": "Apache-2.0",
	"files": [
		"CHANGELOG.md",
		"LICENSE",
		"README.md",
		"dist",
		"src"
	],
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"scripts": {
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"build": "tsc --build",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"test": "vitest run"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeunhwaJeong/haneul-ts-sdks.git"
	},
	"bugs": {
		"url": "https://github.com/MystenLabs/mvr/issues/new"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@haneullabs/build-scripts": "workspace:*",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@haneullabs/haneul": "workspace:*",
		"@types/node": "^24.10.1",
		"enquirer": "^2.4.1",
		"glob": "^13.0.0",
		"prettier": "^3.7.3"
	},
	"sideEffects": false,
	"bin": "./bin/parser.js"
}
