{
	"name": "@haneullabs/utils",
	"version": "0.1.0",
	"description": "Shared utilities for @haneullabs/* packages",
	"license": "Apache-2.0",
	"author": "Haneul Labs <build@haneullabs.com>",
	"sideEffects": false,
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"files": [
		"CHANGELOG.md",
		"dist",
		"src"
	],
	"scripts": {
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"dev": "pnpm build --watch",
		"build": "build-package",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GeunhwaJeong/haneul-ts-sdks.git"
	},
	"keywords": [
		"utils"
	],
	"bugs": {
		"url": "https://github.com/GeunhwaJeong/haneul-ts-sdks/issues"
	},
	"homepage": "https://github.com/GeunhwaJeong/haneul-ts-sdks/tree/main/packages/utils#readme",
	"devDependencies": {
		"@haneullabs/build-scripts": "workspace:*",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	},
	"dependencies": {
		"@scure/base": "^1.2.6"
	}
}
