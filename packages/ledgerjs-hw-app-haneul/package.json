{
	"name": "@haneullabs/ledgerjs-hw-app-haneul",
	"version": "0.7.0",
	"description": "Ledger Hardware Wallet Haneul Application API",
	"keywords": [
		"Ledger",
		"LedgerWallet",
		"LedgerSigner",
		"Haneul",
		"NanoS",
		"Blue",
		"Hardware Wallet"
	],
	"author": "Haneul Labs <build@haneullabs.com>",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "github.com:HaneulLabs/sui.git"
	},
	"bugs": {
		"url": "https://github.com/HaneulLabs/ts-sdks/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/GeunhwaJeong/haneul-ts-sdks/tree/main/packages/ledgerjs-hw-app-haneul#readme",
	"files": [
		"CHANGELOG.md",
		"dist",
		"src"
	],
	"type": "commonjs",
	"main": "./dist/cjs/Haneul.js",
	"module": "./dist/esm/Haneul.js",
	"types": "./dist/cjs/Haneul.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/Haneul.js",
			"require": "./dist/cjs/Haneul.js"
		}
	},
	"size-limit": [
		{
			"path": "dist/Haneul.js",
			"limit": "10 kB"
		}
	],
	"scripts": {
		"analyze": "size-limit",
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"build": "build-package",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"prettier:fix:watch": "onchange '**' -i -f add -f change -j 5 -- prettier -w --ignore-unknown {{file}}",
		"size": "size-limit",
		"test": "pnpm test:typecheck && pnpm test:unit",
		"test:typecheck": "tsc -p ./tests",
		"test:unit": "vitest run",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@ledgerhq/devices": "^8.6.1",
		"@ledgerhq/errors": "^6.26.0",
		"@ledgerhq/hw-bolos": "^6.32.8",
		"@ledgerhq/hw-transport": "^6.31.10",
		"@ledgerhq/ledger-cal-service": "^1.6.0",
		"@ledgerhq/ledger-trust-service": "^0.3.14",
		"fast-sha256": "^1.3.0",
		"semver": ">=7.5.2"
	},
	"devDependencies": {
		"@ledgerhq/hw-transport-mocker": "^6.29.13",
		"@haneullabs/build-scripts": "workspace:*",
		"@size-limit/preset-small-lib": "^12.0.0",
		"@types/node": "^24.10.1",
		"@types/semver": "^7.7.1",
		"size-limit": "^12.0.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15"
	}
}
