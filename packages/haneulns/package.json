{
	"name": "@haneullabs/haneulns",
	"author": "Haneul Labs <build@haneullabs.com>",
	"description": "SuiNS SDK",
	"version": "0.9.13",
	"license": "Apache-2.0",
	"private": false,
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"keywords": [
		"Sui",
		"suins",
		"haneullabs"
	],
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"sideEffects": false,
	"files": [
		"CHANGELOG.md",
		"dist",
		"src"
	],
	"scripts": {
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"build": "build-package",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"test:e2e": "vitest run"
	},
	"engines": {
		"node": ">=16"
	},
	"dependencies": {
		"@haneullabs/sui": "workspace:*",
		"axios": "^1.13.2",
		"axios-retry": "^4.5.0"
	},
	"devDependencies": {
		"@haneullabs/build-scripts": "workspace:*",
		"@types/tmp": "^0.2.6",
		"ts-retry-promise": "^0.8.1",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^4.0.15"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/HaneulLabs/ts-sdks.git"
	},
	"bugs": {
		"url": "https://github.com/haneullabs/suins-contracts/issues"
	}
}
